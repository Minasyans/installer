{
    "name": "minasyans/installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Minasyan",
            "email": "artminasyanartwork@gmail.com",
            "homepage": "https://github.com/minasyans"
        }
    ],
    "homepage": "https://github.com/minasyans/installer",
    "keywords": ["Laravel", "Installer"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Minasyans\\Installer\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Minasyans\\Installer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minasyans\\Installer\\InstallerServiceProvider"
            ],
            "aliases": {
                "Installer": "Minasyans\\Installer\\Facades\\Installer"
            }
        }
    }
}
